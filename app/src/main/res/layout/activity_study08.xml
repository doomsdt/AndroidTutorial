<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_study08"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.soapcopy.tutorial.Study08"
    >

    <LinearLayout
        android:id="@+id/s08_ll0"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <Button
            android:id="@+id/study08_bt1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1"/>

        <Button
            android:id="@+id/study08_bt2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="2"/>

        <Button
            android:id="@+id/study08_bt3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="3"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/s08_ll1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/s08_ll0">

        <Button
            android:id="@+id/study08_bt4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="4"/>

        <Button
            android:id="@+id/study08_bt5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="5"/>

        <Button
            android:id="@+id/study08_bt6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="6"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/s08_ll2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/s08_ll1">

        <Button
            android:id="@+id/study08_bt7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="7"/>

        <Button
            android:id="@+id/study08_bt8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="8"/>

        <Button
            android:id="@+id/study08_bt9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="9"/>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/s08_ll3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/s08_ll2">

        <Button
            android:id="@+id/study08_bt10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="10"/>

        <Button
            android:id="@+id/study08_bt11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="11"/>

        <Button
            android:id="@+id/study08_bt12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="12"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        >

        <TextView
            android:id="@+id/s08_tv_row"
            android:layout_width="0dp"
            android:layout_weight="4"
            android:layout_height="wrap_content" />

        <TextView
            android:id="@+id/s08_tv_col"
            android:layout_width="0dp"
            android:layout_weight="4"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/s08_bt_close"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:text="@string/tuto_close"/>


    </LinearLayout>

</RelativeLayout>
